{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "diffEditor.ignoreTrimWhitespace": false,
  "ruby.specFocusTerminal": false,
  "ruby.lint": {
    "rubocop": false
  },
  "[ruby]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
  },
  "workbench.editor.customLabels.patterns": {
    "**/app/**/_ui.tsx": "(${dirname})/_ui.${extname}",
    "**/app/**/page.tsx": "(${dirname})/page.${extname}",
    "**/app/**/layout.tsx": "(${dirname})/layout.${extname}",
    "**/app/**/index.tsx": "(${dirname})/index.${extname}"
},
  "typescript.workspaceSymbols.scope": "currentProject",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "c"],
      "commands": [
        {
          "command": "alternate.createAlternateFileInSplit"
        }
      ]
    },
    {
      "before": ["<leader>", "o"],
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.closeEditorsInOtherGroups",
        "workbench.action.closeOtherEditors",
        "alternate.alternateFileInSplit"
      ]
    },
    {
      "before": ["<leader>", "O"],
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.closeEditorsInOtherGroups",
        "workbench.action.closeOtherEditors"
      ]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": [
        "extension.runSpecLine",
        "workbench.action.focusActiveEditorGroup"
      ]
    },
    {
      "before": ["<leader>", "f"],
      "commands": [
        "extension.runFileSpecs",
        "workbench.action.focusActiveEditorGroup"
      ]
    },
    {
      "before": ["<leader>", "l"],
      "commands": [
        "extension.runLastSpec",
        "workbench.action.focusActiveEditorGroup"
      ]
    }
  ],
  "vim.foldfix": true,
  "vim.leader": " ",
  "explorer.confirmDragAndDrop": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.colorCustomizations": {
    "[Material Theme High Contrast]": {}
  },
  "workbench.editor.wrapTabs": true,
  "editor.suggest.showStatusBar": true,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "scss.lint.unknownAtRules": "ignore",
  "workbench.colorTheme": "Material Theme High Contrast",
  "terminal.integrated.scrollback": 100000,
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/.next": true,
    "**/build": true,
    "**/coverage": true,
    "**/log": true,
    "app/assets/builds": true,
    "public/assets": true,
    "public/packs": true,
    "public/packs-test": true,
    "spec/cassettes": true,
    "tmp/cache": true
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": true
  },
  "files.autoSave": "onFocusChange",
  "[erb]": {
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.stickyScroll.enabled": true,
  "workbench.editor.enablePreview": false,
  "gitlens.codeLens.enabled": false,
  "files.associations": {
    "*.html.erb": "erb",
    "*.jb": "ruby"
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "extensions.ignoreRecommendations": true,
  "tailwindDocs.useSimpleBrowser": false,
  "headwind.runOnSave": false,
  "remove-tabs-on-save.ignoreFileExtensions": [
    "*.go"
  ],
  "explorer.excludeGitIgnore": true,
  "githubPullRequests.terminalLinksHandler": "github",
  "jest.runMode": "deferred",
  "cSpell.userWords": [
    "healthie"
  ],
  "workbench.settings.editor": "json",
  "playwright.showTrace": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "prettier.enableDebugLogs": true,
  "jest.debugMode": true,
  "jest.jestCommandLine": "pnpm jest",
  "jest.rootPath": "/Users/compuper/workspace/clarity/packages/shared",
  "datadog.timeRange.setup.timeFrame": "1mo",
  "datadog.telemetry.setup.enableTelemetry": "disabled",
  "playwright.reuseBrowser": false,
  "terminal.external.osxExec": "iTerm.app",
}
